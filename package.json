{
  "name": "@coji/kysely-duckdb-wasm",
  "version": "0.1.6",
  "description": "kysely dialect for duckdb wasm",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "all": "npm run check && npm run test && npm run build && npm run docs"
  },
  "keywords": [
    "kysely",
    "kysely-dialect",
    "duckdb",
    "duckdb-wasm"
  ],
  "author": "coji",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@vitest/browser": "3.2.4",
    "dprint": "^0.50.0",
    "playwright": "1.55.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "*",
    "apache-arrow": "*",
    "kysely": "^0.28"
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none"
  }
}
