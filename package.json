{
  "name": "@coji/kysely-duckdb-wasm",
  "version": "0.1.6",
  "description": "kysely dialect for duckdb wasm",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "format": "prettier . --check --experimental-cli",
    "format:fix": "prettier . --write --experimental-cli",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "validate": "pnpm run format && pnpm run lint && pnpm run typecheck && pnpm run test:run && pnpm run build && pnpm run docs"
  },
  "keywords": [
    "kysely",
    "kysely-dialect",
    "duckdb",
    "duckdb-wasm"
  ],
  "author": "coji",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@duckdb/duckdb-wasm": "1.29.1-dev283.0",
    "@types/node": "^24.3.1",
    "@vitest/browser": "3.2.4",
    "apache-arrow": "17.0.0",
    "playwright": "1.55.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.2.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "*",
    "apache-arrow": "*",
    "kysely": "^0.28"
  }
}
