{
  "name": "@coji/kysely-duckdb-wasm",
  "version": "0.2.0",
  "description": "kysely dialect for duckdb wasm",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --sourcemap",
    "format": "prettier . --check --experimental-cli",
    "format:fix": "prettier . --write --experimental-cli",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "validate": "pnpm run format && pnpm run lint && pnpm run typecheck && pnpm run test:run && pnpm run build && pnpm run docs"
  },
  "keywords": [
    "kysely",
    "kysely-dialect",
    "duckdb",
    "duckdb-wasm"
  ],
  "author": "coji",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coji/kysely-duckdb-wasm.git"
  },
  "homepage": "https://github.com/coji/kysely-duckdb-wasm#readme",
  "bugs": {
    "url": "https://github.com/coji/kysely-duckdb-wasm/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@duckdb/duckdb-wasm": "1.31.0",
    "@types/node": "^24.9.2",
    "@vitest/browser": "4.0.6",
    "@vitest/browser-playwright": "4.0.6",
    "apache-arrow": "21.1.0",
    "playwright": "1.56.1",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "peerDependencies": {
    "@duckdb/duckdb-wasm": "*",
    "apache-arrow": "*",
    "kysely": "^0.28"
  },
  "pnpm": {
    "overrides": {
      "apache-arrow": "21.1.0"
    }
  }
}
